{
	"name": "voidCommand",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"start":"npx ts-node --esm source/cli.tsx",
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"figlet": "^1.7.0",
		"ink": "^5.0.1",
		"ink-box": "^2.0.0",
		"ink-text-input": "^6.0.0",
		"meow": "^13.2.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^6.0.0",
		"@types/figlet": "^1.5.8",
		"@types/ink-box": "^1.0.4",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^6.1.3",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^4.0.0",
		"prettier": "^3.3.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.59.3"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
